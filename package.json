    "@parcel/graph": "2.2.1",
    "@parcel/hash": "2.2.1",
    "@parcel/logger": "2.2.1",
    "@parcel/markdown-ansi": "2.2.1",
    "@parcel/namer-default": "2.2.1",
    "@parcel/node-libs-browser": "2.2.1",
    "@parcel/node-resolver-core": "2.2.1",
    "@parcel/optimizer-cssnano": "2.2.1",
