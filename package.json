{
  "name": "kipperweb",
  "version": "0.4.0",
  "description": "The website and documentation for the Kipper programming language",
  "keywords": [
    "website",
    "language",
    "documentation",
    "kipper"
  ],
  "license": "GPL-3.0",
  "author": "Luna Klatzer",
  "engines": {
    "node": ">=16.0.0",
    "pnpm": "8"
  },
  "scripts": {
    "build": "ts-node ./tools/build/build-webpages.ts",
    "postbuild": "pnpm run tslint:fix",
    "parcel-dev-build": "parcel build ./build/index.html --no-content-hash --no-optimize --no-scope-hoist",
    "prod-build": "pnpm run build --prod",
    "postprod-build": "pnpm run postbuild",
    "gh-pages-build": "pnpm run parcel-dev-build --public-url https://kipper-lang.org",
    "watch": "parcel watch src/index.html",
    "start": "pnpm run dev",
    "lint": "pnpm run tslint && pnpm run prettier",
    "lint:fix": "pnpm run tslint:fix && pnpm run prettier",
    "prettier": "prettier --write ./src/ && prettier --write ./tools/",
    "prettier-build": "prettier --write ./build/",
    "tslint": "eslint ./src/ --ext .ts --config .eslintrc",
    "tslint:fix": "eslint ./src/ --ext .ts --config .eslintrc --fix",
    "posttest": "pnpm run lint & pnpm run prettier",
    "predev": "pnpm run build",
    "dev": "http-server ./dist -p 1234 -e html --cors"
  },
  "devDependencies": {
    "@parcel/babylon-walk": "2.2.1",
    "@parcel/bundler-default": "2.11.0",
    "@parcel/cache": "2.11.0",
