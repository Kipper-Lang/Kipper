    "@parcel/fs": "2.11.0",
    "@parcel/fs-search": "2.9.3",
    "@parcel/fs-write-stream-atomic": "2.6.0",
    "@parcel/graph": "2.9.3",
    "@parcel/hash": "2.9.3",
    "@parcel/logger": "2.11.0",
    "@parcel/markdown-ansi": "2.11.0",
    "@parcel/namer-default": "2.11.0",
    "@parcel/node-libs-browser": "2.2.1",
    "@parcel/node-resolver-core": "2.2.1",
    "@parcel/optimizer-cssnano": "2.11.0",
