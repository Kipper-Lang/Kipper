{
  "name": "kipperweb",
  "version": "0.3.0",
  "description": "The website for the Kipper programming lang",
  "keywords": [
    "website",
    "language",
    "documentation",
    "kipper"
  ],
  "license": "GPL-3.0",
  "author": "Luna Klatzer & Maxmilian Seebacher",
  "engines": {
    "node": ">= 16.0.0",
    "pnpm": ">=7"
  },
  "scripts": {
    "prebuild": "ts-node ./tools/build/build-webpages.ts && pnpm run tslint:fix",
    "build": "parcel build ./build/index.html --no-content-hash --no-optimize --no-scope-hoist",
    "gh-pages-build": "parcel build build/index.html --public-url https://wmc-ahif-2021.github.io/Kipper-Web/ --dist-dir . --no-content-hash --no-optimize --no-scope-hoist",
    "watch": "parcel watch src/index.html",
    "start": "pnpm run build && pnpm run dev",
    "lint": "pnpm run tslint && pnpm run prettier",
    "lint:fix": "pnpm run tslint:fix && pnpm run prettier",
    "prettier": "prettier --write ./src/ && prettier --write ./tools/",
    "tslint": "eslint ./src/ --ext .ts --config .eslintrc",
    "tslint:fix": "eslint ./src/ --ext .ts --config .eslintrc --fix",
    "posttest": "pnpm run lint & pnpm run prettier",
    "dev": "parcel ./build/index.html --open --no-cache"
  },
  "devDependencies": {
