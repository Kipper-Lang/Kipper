const numList: list<num> = [1, 2, 3, 4];

/* Calculates the sum of all items stored inside 'arr'. */
def getSumOf(arr: list<num>) -> num {
  var sum: num = 0;
  for (var i: num = 0; i < len(arr); i++) {
    sum += arr[i];
  }
  return sum;
}

print(f"List: {numList}");

/* Calculating the sum of the items in the list */
var result: num = getSumOf(numList);
print(f"Result: {result as str}");

/* Exiting the program */
process.exit(0);
